
<!-- Start: Page Banner -->
<section class="page-banner services-banner">
    <div class="container">
        <div class="banner-header">
            <h2>ISBN</h2>
            <span class="underline center"></span>
            <p class="lead"></p>
        </div>
        <div class="breadcrumb">
            <ul>
                <li><a href="/">Beranda</a></li>
                <li>ISBN</li>
            </ul>
        </div>
    </div>
</section>
<!-- End: Page Banner -->
<!-- Start: Cart Section -->
<div id="content" class="site-content">
    <div id="primary" class="content-area">
        <main id="main" class="site-main">
            <div class="signin-main">
                <div class="container">
                    <div class="woocommerce">
                        <div class="woocommerce-login">
                            <div class="company-info signin-register">
                                <div class="col-md-5 border-dark new-user">
                                    <div class="row">
                                        <div class="col-md-12">
                                            <div class="company-detail new-account bg-light margin-right">
                                                <div class="new-user-head">
                                                    <h2>ISBN</h2>
                                                    <span class="underline left"></span>
                                                </div>
                                                <form class="login" method="post" action="/isbn_submit">
                                                    <p class="form-row form-row-first input-required">
                                                        <label>
                                                            <span class="first-letter">Judul Buku</span>
                                                        </label>
                                                        <input type="text" id="judul" name="judul" class="input-text">
                                                    </p>
                                                    <p class="form-row input-required">
                                                        <label>
                                                            <span class="first-letter">Penerjemah</span>
                                                        </label>
                                                        <input type="text" id="penerjemah" name="penerjemah" class="input-text">
                                                    </p>
                                                    <p class="form-row input-required">
                                                        <label>
                                                            <span class="first-letter">Edisi</span>
                                                        </label>
                                                        <input type="text" id="edisi" name="edisi" class="input-text">
                                                    </p>
                                                    <p class="form-row input-required">
                                                        <label>
                                                            <span class="first-letter">Seri</span>
                                                        </label>
                                                        <input type="text" id="seri" name="seri" class="input-text">
                                                    </p>
                                                    <p class="form-row input-required">
                                                        <label>
                                                            <span class="first-letter">Tahun Terbit</span>
                                                        </label>
                                                        <input type="text" id="tahun" name="tahun" class="input-text">
                                                    </p>
                                                    <p class="form-row input-required">
                                                        <label>
                                                            <span class="first-letter">Jumlah Halaman</span>
                                                        </label>
                                                        <input type="text" id="jumlahHal" name="jumlahHal" class="input-text">
                                                    </p>
                                                    <p class="form-row input-required">
                                                        <label>
                                                            <span class="first-letter">Tinggi Buku</span>
                                                        </label>
                                                        <input type="text" id="tinggiB" name="tinggiB" class="input-text">
                                                    </p>
                                                    <p class="form-row input-required">
                                                        <label>
                                                            <span class="first-letter">Kategory</span>
                                                        </label>
                                                        <select name="kategory" id="kategory" class="form-control">
                                                            <option value="">-pilih kategori</option>
                                                            {{#each results}}
                                                            <option value="{{this.kategori_koleksi}}">{{this.kategori_koleksi}}</option>
                                                            {{/each}}
                                                        </select>
                                                    </p>
                                                    <p class="form-row input-required">
                                                        <label>
                                                            <span class="first-letter">Jenis</span>
                                                        </label>
                                                        <input type="text" id="jenis" name="jenis" class="input-text">
                                                    </p>
                                                    <p class="form-row input-required">
                                                        <label>
                                                            <span class="first-letter">Media</span>
                                                        </label>
                                                        <input type="text" id="media" name="media" class="input-text">
                                                    </p>
                                                    <p class="form-row input-required">
                                                        <label>
                                                            <span class="first-letter">File Buku</span>
                                                        </label>
                                                    </p>
                                                    <p>
                                                        <input type="file" id="file_buku" name="file_buku" class="input-text">
                                                    </p>
                                                    <div class="clear"></div>
                                                    <input type="submit" value="Submit" name="submit" class="button btn btn-default">
                                                    <div class="clear"></div>
                                                </form>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </main>
    </div>
</div>
<!-- End: Cart Section -->